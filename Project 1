","    outliers_prob = []\n","    outliers_poss = []\n","    for index, x in enumerate(df[variable]):\n","        if x <= outer_fence_le or x >= outer_fence_ue:\n","            outliers_prob.append(index)\n","    for index, x in enumerate(df[variable]):\n","        if x <= inner_fence_le or x >= inner_fence_ue:\n","            outliers_poss.append(index)\n","    return outliers_prob, outliers_poss\n"],"metadata":{"papermill":{"duration":0.108455,"end_time":"2022-02-23T02:27:37.524004","exception":false,"start_time":"2022-02-23T02:27:37.415549","status":"completed"},"tags":[],"execution":{"iopub.status.busy":"2022-05-15T12:18:18.34011Z","iopub.execute_input":"2022-05-15T12:18:18.34147Z","iopub.status.idle":"2022-05-15T12:18:18.351008Z","shell.execute_reply.started":"2022-05-15T12:18:18.341428Z","shell.execute_reply":"2022-05-15T12:18:18.349793Z"},"trusted":true,"id":"zEB1bIAotWHe"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["random_building_id = df.groupby(\"building_id\").get_group(108)\n","\n","probable_outliers_tm, possible_outliers_tm = tukeys_method(random_building_id, \"meter_reading\")\n","\n","print(probable_outliers_tm)\n","print(\"*****************************************************************************************\")\n","print(possible_outliers_tm)"],"metadata":{"execution":{"iopub.status.busy":"2022-05-15T12:18:18.353759Z","iopub.execute_input":"2022-05-15T12:18:18.354139Z","iopub.status.idle":"2022-05-15T12:18:18.507396Z","shell.execute_reply.started":"2022-05-15T12:18:18.354092Z","shell.execute_reply":"2022-05-15T12:18:18.50607Z"},"trusted":true,"id":"1rEqgs4ctWHf"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["len(probable_outliers_tm)"],"metadata":{"papermill":{"duration":0.106331,"end_time":"2022-02-23T02:27:37.952214","exception":false,"start_time":"2022-02-23T02:27:37.845883","status":"completed"},"tags":[],"execution":{"iopub.status.busy":"2022-05-15T12:18:18.50881Z","iopub.execute_input":"2022-05-